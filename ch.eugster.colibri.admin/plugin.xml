<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="ch.eugster.colibri.admin.application"
         name="ColibriTS II"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="ch.eugster.colibri.admin.app.AdminApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="ch.eugster.colibri.admin.perspective.toolbar">
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
